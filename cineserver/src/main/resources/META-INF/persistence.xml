<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">
	
	<persistence-unit name="cineserver" >
		<class>br.com.codifico.model.Cinema</class>
		<class>br.com.codifico.model.Filme</class>
		<class>br.com.codifico.model.Endereco</class>
		<class>br.com.codifico.model.apresentacao.FilmeCartaz</class>
		<class>br.com.codifico.model.apresentacao.EnderecoResumo</class>
		<class>br.com.codifico.model.element.CinemaElement</class>
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect" />
		</properties>
	</persistence-unit>

</persistence>


